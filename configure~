	fi



# Check if device aware MPI is requested
if (test "x${have_cuda}" = xyes ||
    test "x${have_hip}" = xyes  ||
    test "x${have_opencl}" = xyes); then
   if test "x${enable_device_mpi}" = xyes; then

      # Check that necessary accelerator modules are loaded
      if (test "x${is_cray}" = xyes || test "x${is_hpe_cray}" = xyes); then

	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking Cray Accelerator Target" >&5
printf %s "checking Cray Accelerator Target... " >&6; }
	if test "${CRAY_ACCEL_TARGET}"; then
	   if test "x${CRAY_ACCEL_TARGET}" = "xhost"; then
	      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	      as_fn_error $? "Invalid accelerator target (host)" "$LINENO" 5
	      have_cray_accel="no"
	   else
              { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
	      have_cray_accel="yes"
	   fi
	else
	   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	   as_fn_error $? "Cray Accelerator Target not found" "$LINENO" 5
	   have_cray_accel="no"
	fi


      fi

      device_mpi=.true.


printf "%s\n" "#define HAVE_DEVICE_MPI 1" >>confdefs.h


      if test "x${have_cuda}" = xyes; then
         CUDA_CFLAGS="$CUDA_CFLAGS -DHAVE_DEVICE_MPI=1"

         # Make sure we have support for atomicAdd
      	 if test "$CUDA_ARCH"
then :

else $as_nop
  CUDA_ARCH="-arch sm_60"
fi
      fi

      if  test "x${have_hip}" = xyes; then
         HIP_HIPCC_FLAGS="$HIP_HIPCC_FLAGS -DHAVE_DEVICE_MPI=1"
      fi
   else
      device_mpi=.false.

   fi
else
   device_mpi=.false.

fi

