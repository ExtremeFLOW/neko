else
  ac_cv_path_EGREP=$EGREP
fi

   fi
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_path_EGREP" >&5
printf "%s\n" "$ac_cv_path_EGREP" >&6; }
 EGREP="$ac_cv_path_EGREP"



	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for a Cray XT, XE, XC system" >&5
printf %s "checking for a Cray XT, XE, XC system... " >&6; }
	ac_ext=c
