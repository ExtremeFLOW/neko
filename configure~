                   opencl_bcknd="1"

printf "%s\n" "#define HAVE_OPENCL 1" >>confdefs.h

		   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
