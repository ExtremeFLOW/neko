printf %s "checking for libxsmmf >= 0.16.1... " >&6; }

if test -n "$libxsmmf_CFLAGS"; then
    pkg_cv_libxsmmf_CFLAGS="$libxsmmf_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"libxsmmf >= 0.16.1\""; } >&5
  ($PKG_CONFIG --exists --print-errors "libxsmmf >= 0.16.1") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_libxsmmf_CFLAGS=`$PKG_CONFIG --cflags "libxsmmf >= 0.16.1" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$libxsmmf_LIBS"; then
    pkg_cv_libxsmmf_LIBS="$libxsmmf_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"libxsmmf >= 0.16.1\""; } >&5
  ($PKG_CONFIG --exists --print-errors "libxsmmf >= 0.16.1") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_libxsmmf_LIBS=`$PKG_CONFIG --libs "libxsmmf >= 0.16.1" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        libxsmmf_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "libxsmmf >= 0.16.1" 2>&1`
        else
	        libxsmmf_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "libxsmmf >= 0.16.1" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$libxsmmf_PKG_ERRORS" >&5

	xsmm_bkcnd="0" have_libxsmm=no
elif test $pkg_failed = untried; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	xsmm_bkcnd="0" have_libxsmm=no
else
	libxsmmf_CFLAGS=$pkg_cv_libxsmmf_CFLAGS
	libxsmmf_LIBS=$pkg_cv_libxsmmf_LIBS
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
	have_libxsmm=yes xsmm_bcknd="1"
fi
	   if test "x${have_libxsmm}" = xyes; then
