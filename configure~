    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_mkl_gf_lp64_sgemm" >&5
printf "%s\n" "$ac_cv_lib_mkl_gf_lp64_sgemm" >&6; }
if test "x$ac_cv_lib_mkl_gf_lp64_sgemm" = xyes
then :
  ax_blas_ok=yes;BLAS_LIBS="-lmkl_gf_lp64 -lmkl_sequential -lmkl_core -lpthread"
fi

		# 32 bit
		elif test $host_cpu = i686; then
			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for sgemm in -lmkl_gf" >&5
printf %s "checking for sgemm in -lmkl_gf... " >&6; }
if test ${ac_cv_lib_mkl_gf_sgemm+y}
then :
  printf %s "(cached) " >&6
else $as_nop
