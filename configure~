ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

	      CC=$HIPCC
	      LIBS=""

	      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for roctxRangePop in -lroctx64" >&5
printf %s "checking for roctxRangePop in -lroctx64... " >&6; }
