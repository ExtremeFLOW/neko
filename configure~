# JSON-Fortran

pkg_failed=no
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for json-fortran >= 7.1.0" >&5
printf %s "checking for json-fortran >= 7.1.0... " >&6; }

if test -n "$JSON_Fortran_CFLAGS"; then
    pkg_cv_JSON_Fortran_CFLAGS="$JSON_Fortran_CFLAGS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"json-fortran >= 7.1.0\""; } >&5
  ($PKG_CONFIG --exists --print-errors "json-fortran >= 7.1.0") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_JSON_Fortran_CFLAGS=`$PKG_CONFIG --cflags "json-fortran >= 7.1.0" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi
if test -n "$JSON_Fortran_LIBS"; then
    pkg_cv_JSON_Fortran_LIBS="$JSON_Fortran_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"json-fortran >= 7.1.0\""; } >&5
  ($PKG_CONFIG --exists --print-errors "json-fortran >= 7.1.0") 2>&5
  ac_status=$?
  printf "%s\n" "$as_me:${as_lineno-$LINENO}: \$? = $ac_status" >&5
  test $ac_status = 0; }; then
  pkg_cv_JSON_Fortran_LIBS=`$PKG_CONFIG --libs "json-fortran >= 7.1.0" 2>/dev/null`
		      test "x$?" != "x0" && pkg_failed=yes
else
  pkg_failed=yes
fi
 else
    pkg_failed=untried
fi



if test $pkg_failed = yes; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }

if $PKG_CONFIG --atleast-pkgconfig-version 0.20; then
        _pkg_short_errors_supported=yes
else
        _pkg_short_errors_supported=no
fi
        if test $_pkg_short_errors_supported = yes; then
	        JSON_Fortran_PKG_ERRORS=`$PKG_CONFIG --short-errors --print-errors --cflags --libs "json-fortran >= 7.1.0" 2>&1`
        else
	        JSON_Fortran_PKG_ERRORS=`$PKG_CONFIG --print-errors --cflags --libs "json-fortran >= 7.1.0" 2>&1`
        fi
	# Put the nasty error message in config.log where it belongs
	echo "$JSON_Fortran_PKG_ERRORS" >&5

	as_fn_error $? "Package requirements (json-fortran >= 7.1.0) were not met:

$JSON_Fortran_PKG_ERRORS

Consider adjusting the PKG_CONFIG_PATH environment variable if you
installed software in a non-standard prefix.

Alternatively, you may set the environment variables JSON_Fortran_CFLAGS
and JSON_Fortran_LIBS to avoid the need to call pkg-config.
See the pkg-config man page for more details." "$LINENO" 5
elif test $pkg_failed = untried; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	{ { printf "%s\n" "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
printf "%s\n" "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "The pkg-config script could not be found or is too old.  Make sure it
is in your PATH or set the PKG_CONFIG environment variable to the full
path to pkg-config.

Alternatively, you may set the environment variables JSON_Fortran_CFLAGS
and JSON_Fortran_LIBS to avoid the need to call pkg-config.
See the pkg-config man page for more details.

To get pkg-config, see <http://pkg-config.freedesktop.org/>.
See \`config.log' for more details" "$LINENO" 5; }
else
	JSON_Fortran_CFLAGS=$pkg_cv_JSON_Fortran_CFLAGS
	JSON_Fortran_LIBS=$pkg_cv_JSON_Fortran_LIBS
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }

fi
LIBS="$JSON_Fortran_LIBS $LIBS"
FCFLAGS="$JSON_Fortran_CFLAGS $FCFLAGS"

