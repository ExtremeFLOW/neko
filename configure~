then :
  have_roctx=yes;ROCTX_LIBS="-lroctx64"
else $as_nop
  have_roctx=no
fi


              if test x"${have_roctx}" = xyes; then
                 roctx_bcknd="1"

printf "%s\n" "#define HAVE_ROCTX 1" >>confdefs.h

                 LIBS="$ROCTX_LIBS $_LIBS"
                 HIP_HIPCC_FLAGS="$HIP_HIPCC_FLAGS $ROCTX_CFLAGS"
              else
