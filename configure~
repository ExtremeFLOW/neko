then :
  ac_cv_lib_parmetis_ParMETIS_V3_PartMeshKway=yes
else $as_nop
  ac_cv_lib_parmetis_ParMETIS_V3_PartMeshKway=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
