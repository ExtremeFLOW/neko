{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_parmetis_ParMETIS_V3_PartMeshKway" >&5
printf "%s\n" "$ac_cv_lib_parmetis_ParMETIS_V3_PartMeshKway" >&6; }
if test "x$ac_cv_lib_parmetis_ParMETIS_V3_PartMeshKway" = xyes
then :
  have_parmetis=yes;PARMETIS_LIBS="-lparmetis -lmetis"
else $as_nop
  have_parmetis=no
fi


	   if test x"${have_parmetis}" = xyes; then
	      ac_ext=c
