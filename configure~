  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $HIPCC" >&5
printf "%s\n" "$HIPCC" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


fi
		fi

                if test "$HIP_HIPCC_FLAGS"
then :

else $as_nop
  HIP_HIPCC_FLAGS="-O3"
fi

