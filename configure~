cat > conftest.$ac_ext <<_ACEOF
      program main
      call sgemm
      end
_ACEOF
if ac_fn_fc_try_link "$LINENO"
then :
  ac_cv_lib_mkl_gf_sgemm=yes
else $as_nop
  ac_cv_lib_mkl_gf_sgemm=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_mkl_gf_sgemm" >&5
printf "%s\n" "$ac_cv_lib_mkl_gf_sgemm" >&6; }
if test "x$ac_cv_lib_mkl_gf_sgemm" = xyes
then :
  ax_blas_ok=yes;BLAS_LIBS="-lmkl_gf -lmkl_sequential -lmkl_core -lpthread"
fi

		fi
	# MKL for other compilers (Intel, PGI, ...?)
	else
		# 64-bit
		if test $host_cpu = x86_64; then
			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for sgemm in -lmkl_intel_lp64" >&5
printf %s "checking for sgemm in -lmkl_intel_lp64... " >&6; }
