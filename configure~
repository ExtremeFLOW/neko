if test ${with_nvtx+y}
then :
  withval=$with_nvtx;
                    if test -d "$withval"; then
                       ac_nvtx_path="$withval";
                       if test -d "$ac_nvtx_path/lib64"
then :
  suffix="64"
else $as_nop
  suffix=""
fi
                       NVTX_LDFLAGS="-L$ac_nvtx_path/lib$suffix"
                       NVTX_CFLAGS="-I$ac_nvtx_path/include"
                    fi

else $as_nop
  with_nvtx=no
fi

        nvtx_bcknd="0"
        if test "x${with_nvtx}" != xno; then
           if test "x${have_cuda}" != xyes; then
              as_fn_error $? "NVTX requires CUDA" "$LINENO" 5
           fi

           if test -d "$ac_nvtx_path"; then
	      CPPFLAGS_SAVED="$CPPFLAGS"
              LDFLAGS_SAVED="$LDFLAGS"
	      CPPFLAGS="$NVTX_CPPFLAGS $CPPFLAGS"
	      LDFLAGS="$NVTX_LDFLAGS $LDFLAGS"
	      export CPPFLAGS
	      export LDFLAGS
	   fi

              _CC=$CC
              _CFLAGS=$CFLAGS
	      _LIBS=$LIBS
	      ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

	      CC=$NVCC
              CFLAGS=""
	      LIBS=""

	      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for nvtxRangePop in -lnvToolsExt" >&5
printf %s "checking for nvtxRangePop in -lnvToolsExt... " >&6; }
if test ${ac_cv_lib_nvToolsExt_nvtxRangePop+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lnvToolsExt  $LIBS"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

/* Override any GCC internal prototype to avoid an error.
   Use char because int might match the return type of a GCC
   builtin and then its argument prototype would still apply.  */
char nvtxRangePop ();
int
main (void)
{
return nvtxRangePop ();
  ;
  return 0;
}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  ac_cv_lib_nvToolsExt_nvtxRangePop=yes
else $as_nop
  ac_cv_lib_nvToolsExt_nvtxRangePop=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_nvToolsExt_nvtxRangePop" >&5
printf "%s\n" "$ac_cv_lib_nvToolsExt_nvtxRangePop" >&6; }
if test "x$ac_cv_lib_nvToolsExt_nvtxRangePop" = xyes
then :
  have_nvtx=yes;NVTX_LIBS="-lnvToolsExt"
else $as_nop
  have_nvtx=no
fi


              if test x"${have_nvtx}" = xyes; then
                 nvtx_bcknd="1"

printf "%s\n" "#define HAVE_NVTX 1" >>confdefs.h

                 LIBS="$NVTX_LIBS $_LIBS"
                 CUDA_CFLAGS="$CUDA_CFLAGS $NVTX_CFLAGS"
              else
                 as_fn_error $? "NVTX not found" "$LINENO" 5
              fi
              CC=$_CC
              CFLAGS=$_CFLAGS
              ac_ext=${ac_fc_srcext-f}
ac_compile='$FC -c $FCFLAGS $ac_fcflags_srcext conftest.$ac_ext >&5'
ac_link='$FC -o conftest$ac_exeext $FCFLAGS $LDFLAGS $ac_fcflags_srcext conftest.$ac_ext $LIBS >&5'
