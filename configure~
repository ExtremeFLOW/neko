	if test "x${with_adios2}" != xno; then
	   PATH_SAVED="$PATH"
	   if test -d "$ac_adios2_path"; then
	      PATH="$ac_adios2_path/bin:$PATH"
	   fi

	   # Extract the first word of "adios2-config", so it can be a program name with args.
set dummy adios2-config; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_ADIOS2CONF+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$ADIOS2CONF"; then
  ac_cv_prog_ADIOS2CONF="$ADIOS2CONF" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_ADIOS2CONF="yes"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
ADIOS2CONF=$ac_cv_prog_ADIOS2CONF
if test -n "$ADIOS2CONF"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ADIOS2CONF" >&5
printf "%s\n" "$ADIOS2CONF" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi



	   if test x"${ADIOS2CONF}" == x"yes"; then
	      ADIOS2_CXXFLAGS=`adios2-config --cxx-flags`
	      CXXFLAGS="$ADIOS2_CXXFLAGS $CXXFLAGS"

	      ADIOS2_LDFLAGS=`adios2-config --cxx-libs`
	      LDFLAGS="$ADIOS2_LDFLAGS $LDFLAGS"
      	      with_adios2=yes
	      have_adios2=yes

	    else
	      with_adios2=no
	    fi
            PATH="$PATH_SAVED"

	fi


