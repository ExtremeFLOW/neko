printf "%s\n" "no" >&6; }
fi


  test -n "$DOXYGEN_DEPS" && break
done

fi
 if test -n "$DOXYGEN_DEPS"; then
  ENABLE_DOXYGEN_TRUE=
  ENABLE_DOXYGEN_FALSE='#'
