      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
      have_pfunit=no
 else
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
      have_pfunit=yes
 fi



  if test "x${have_pfunit}" = xyes; then
  ENABLE_PFUNIT_TRUE=
