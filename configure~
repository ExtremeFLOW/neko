then :
  ac_cv_lib_mkl_intel_sgemm=yes
else $as_nop
  ac_cv_lib_mkl_intel_sgemm=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_mkl_intel_sgemm" >&5
printf "%s\n" "$ac_cv_lib_mkl_intel_sgemm" >&6; }
