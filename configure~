if ac_fn_fc_try_link "$LINENO"
then :
  ac_cv_lib_metis_METIS_PartGraphKway=yes
else $as_nop
  ac_cv_lib_metis_METIS_PartGraphKway=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
LIBS=$ac_check_lib_save_LIBS
fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ac_cv_lib_metis_METIS_PartGraphKway" >&5
printf "%s\n" "$ac_cv_lib_metis_METIS_PartGraphKway" >&6; }
if test "x$ac_cv_lib_metis_METIS_PartGraphKway" = xyes
then :
  have_metis=yes;METIS_LIBS="-lmetis"
else $as_nop
  have_metis=no
fi


	   if test x"${have_metis}" = xyes; then

printf "%s\n" "#define HAVE_METIS 1" >>confdefs.h

	   else
	      if test -d "$ac_metis_path"; then
	      	 LDFLAGS="$LDFLAGS_SAVED"
	      fi
	   fi
	fi





# Check whether --with-parmetis was given.
if test ${with_parmetis+y}
then :
  withval=$with_parmetis;
	if test -d "$withval"; then
		ac_parmetis_path="$withval";
		PARMETIS_LDFLAGS="-L$ac_parmetis_path/lib"
		PARMETIS_CPPFLAGS="-I$ac_parmetis_path/include"
	fi

else $as_nop
  with_parmetis=no
fi





# Check whether --with-parmetis-libdir was given.
if test ${with_parmetis_libdir+y}
then :
  withval=$with_parmetis_libdir;
	if test -d "$withval"; then
	   ac_parmetis_libdir="$withval"
	fi

fi


	if test -d "$ac_parmetis_libdir"; then
	    PARMETIS_LDFLAGS="-L$ac_parmetis_libdir"
        fi

	if test -d "$ac_parmetis_path"; then
	   CPPFLAGS_SAVED="$CPPFLAGS"
	   LDFLAGS_SAVED="$LDFLAGS"
	   CPPFLAGS="$PARMETIS_CPPFLAGS $CPPFLAGS"
	   LDFLAGS="$PARMETIS_LDFLAGS $LDFLAGS"
	   export CPPFLAGS
	   export LDFLAGS
	fi
	if test "x${with_parmetis}" != xno; then
	   ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

	   ac_fn_c_check_header_compile "$LINENO" "parmetis.h" "ac_cv_header_parmetis_h" "$ac_includes_default"
if test "x$ac_cv_header_parmetis_h" = xyes
then :
  have_parmetis_h=yes
else $as_nop
  have_parmetis_h=no
fi

	   if test x"${have_parmetis_h}" = xno; then
		if test -d "$ac_parmetis_path"; then
		   CPPFLAGS="$CPPFLAGS_SAVED"
		fi
	   fi
	   ac_ext=${ac_fc_srcext-f}
ac_compile='$FC -c $FCFLAGS $ac_fcflags_srcext conftest.$ac_ext >&5'
ac_link='$FC -o conftest$ac_exeext $FCFLAGS $LDFLAGS $ac_fcflags_srcext conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_fc_compiler_gnu


	   { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for ParMETIS_V3_PartMeshKway in -lparmetis" >&5
printf %s "checking for ParMETIS_V3_PartMeshKway in -lparmetis... " >&6; }
if test ${ac_cv_lib_parmetis_ParMETIS_V3_PartMeshKway+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  ac_check_lib_save_LIBS=$LIBS
LIBS="-lparmetis -lmetis $LIBS"
cat > conftest.$ac_ext <<_ACEOF
      program main
      call ParMETIS_V3_PartMeshKway
      end
_ACEOF
