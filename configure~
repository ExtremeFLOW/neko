# Select Working precision of REAL
if test "x$enable_real" = xsp; then
   NEKO_REAL_TYPE="sp"

   NEKO_C_REAL_TYPE="c_float"

   NEKO_MPI_REAL_TYPE="MPI_REAL"

   NEKO_DEV_REAL_TYPE="float"

elif test "x$enable_real" = xdp; then
   NEKO_REAL_TYPE="dp"

   NEKO_C_REAL_TYPE="c_double"

   NEKO_MPI_REAL_TYPE="MPI_DOUBLE_PRECISION"

   NEKO_DEV_REAL_TYPE="double"

elif test "x$enable_real" = xqp; then
   NEKO_REAL_TYPE="qp"

   NEKO_C_REAL_TYPE="c_long_double"

   NEKO_MPI_REAL_TYPE="MPI_REAL16"

   NEKO_DEV_REAL_TYPE="long double"

else
   as_fn_error $? "Invalid working precision" "$LINENO" 5
fi

if test "x$have_real128" = xyes; then
   NEKO_REAL128_TYPE="REAL128"

else
   NEKO_REAL128_TYPE="selected_real_kind(1)"

   if test "x$enable_real" = xqp; then
      as_fn_error $? "REAL128 not supported" "$LINENO" 5
   fi
fi

