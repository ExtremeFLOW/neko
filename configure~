printf "%s\n" "#define HAVE_PARMETIS 1" >>confdefs.h

	   else
	      if test -d "$ac_parmetis_path"; then
	      	 LDFLAGS="$LDFLAGS_SAVED"
	      fi
	   fi
	fi


