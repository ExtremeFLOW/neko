{
  "version": 1.0,
  "case":
  {
  "mesh_file": "512.nmsh",
  "output_boundary": true,
  "output_checkpoints": false,
  "output_at_end": true,
  "load_balance": false,
  "job_timelimit": "00:00:00",
  "end_time": 2e-2,
  "timestep": 1e-2,
  "numerics": {
    "time_order": 3,
    "polynomial_order": 7,
    "dealias": true
  },
  "fluid": {
    "scheme": "pnpn",
    "Re": 360,
    "nut_field" : "nut",
    "initial_condition": {
      "type": "user"
    },
    "velocity_solver": {
      "type": "coupledcg",
      "preconditioner": "jacobi",
      "projection_space_size": 0,
      "absolute_tolerance": 1e-7,
      "max_iterations": 800
    },
    "pressure_solver": {
      "type": "gmres",
      "preconditioner": "hsmg",
      "projection_space_size": 20,
      "absolute_tolerance": 1e-4,
      "max_iterations": 800
    },
    "output_control": "simulationtime",
    "output_value": 1 
  },
  "simulation_components": 
  [
    {
      "type": "vorticity",
      "compute_control": "tsteps",
      "compute_value": 50
    },
    {
      "type": "les_model",
      "model" : "my_model",
      "nut_field": "nut",
      "output_control": "global",
      "output_filename": "les"
    }
  ]
  }
}
