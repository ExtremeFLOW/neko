@test
subroutine test_interpolation_quad
  use pfunit
  use num_types, only : i4, rp
  use subset_interpolation, only : subset_interpolation_t
  use subset_interpolation_quad, only : subset_intp_quad_init
  implicit none
  class(subset_interpolation_t), allocatable :: quad
  integer(i4) :: hng

  hng = 1 ! other values should be tasted as well
  call subset_intp_quad_init(hng, quad)

  ! check hanging info
  @assertEqual(quad%hng(), hng)
  ! check dummy operation flag
  @assertFalse(quad%ifdmy())

  ! missing tests for jmat initialisation and interpolation

  call quad%free_jmat()
  deallocate(quad)

end subroutine test_interpolation_quad
