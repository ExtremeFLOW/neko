@test
subroutine test_interpolation_quad
  use pfunit
  use num_types, only : i4, dp
  use ncnf_interpolation, only : ncnf_interpolation_t
  use ncnf_interpolation_quad, only : ncnf_intp_quad_init
  implicit none
  class(ncnf_interpolation_t), allocatable :: quad
  integer(i4) :: hng

  hng = 1
  call ncnf_intp_quad_init(hng, quad)

  ! check hanging info
  @assertEqual(quad%hng(), hng)

  ! missing tests for jmat initialisation and interpolation

  call quad%free_jmat()
  deallocate(quad)

end subroutine test_interpolation_quad
